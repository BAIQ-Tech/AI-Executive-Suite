{
  "dashboard": {
    "id": null,
    "title": "AI Executive Suite - Business Metrics",
    "tags": ["ai-executive-suite", "business"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Daily Active Users",
        "type": "stat",
        "targets": [
          {
            "expr": "increase(user_sessions_total[24h])",
            "legendFormat": "Daily Active Users"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Decisions Created Today",
        "type": "stat",
        "targets": [
          {
            "expr": "increase(decisions_created_total[24h])",
            "legendFormat": "Decisions Today"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Documents Processed Today",
        "type": "stat",
        "targets": [
          {
            "expr": "increase(documents_processed_total[24h])",
            "legendFormat": "Documents Today"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "AI Response Accuracy",
        "type": "gauge",
        "targets": [
          {
            "expr": "avg(ai_decision_accuracy_score)",
            "legendFormat": "Accuracy Score"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 1,
            "unit": "percentunit",
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 0.7},
                {"color": "green", "value": 0.85}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "Decisions by Executive Type",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (executive_type) (increase(decisions_created_total[24h]))",
            "legendFormat": "{{executive_type}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 6,
        "title": "Decision Confidence Scores",
        "type": "histogram",
        "targets": [
          {
            "expr": "histogram_quantile(0.5, decision_confidence_score_bucket)",
            "legendFormat": "Median"
          },
          {
            "expr": "histogram_quantile(0.95, decision_confidence_score_bucket)",
            "legendFormat": "95th percentile"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 7,
        "title": "AI Response Time Trend",
        "type": "graph",
        "targets": [
          {
            "expr": "avg(ai_response_duration_seconds)",
            "legendFormat": "Average Response Time"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 8,
        "title": "Document Processing Success Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(documents_processed_success_total[5m]) / rate(documents_processed_total[5m])",
            "legendFormat": "Success Rate"
          }
        ],
        "yAxes": [
          {
            "label": "Percentage",
            "min": 0,
            "max": 1
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      }
    ],
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "refresh": "1m"
  }
}